    "chart.js": "^2.9.3",
    "connected-react-router": "^6.6.1",
    "date-fns": "^2.8.1",
    "env-cmd": "^10.0.1",
    "express": "^4.17.1",
    "formik": "^2.0.3",
    "gh-pages": "^2.1.1",
    "history": "^4.10.1",
    "immer": "^5.0.0",
    "qs": "^6.9.1",
    "query-string": "^6.9.0",
    "react": "^16.11.0",
    "react-chartjs-2": "^2.8.0",
    "react-cookie": "^4.0.1",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.0.0",
    "styled-media-query": "^2.1.2",
    "styled-reset-advanced": "^1.0.3",
    "typescript": "3.6.4",
    "uuid": "^3.3.3",
    "yup": "^0.27.0"
  },
  "scripts": {
    "start": "run-p --race start:server start:app",
    "start:app": "env-cmd -f .env.local react-scripts start",
    "start:server": "cd server && ts-node-dev --respawn --transpileOnly ./server.ts && wait-on tcp:5000",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix './src/**/*.{ts,tsx}'",
    "build": "react-scripts build",
    "build:staging": "env-cmd -f .env.staging npm run build",
    "test": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "babel": {
    "plugins": [
      [
        "babel-plugin-styled-components"
      ]
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-info": "^5.2.5",
    "@storybook/addon-knobs": "^5.2.5",
    "@storybook/addon-links": "^5.2.5",
    "@storybook/addon-storysource": "^5.2.5",
    "@storybook/addon-viewport": "^5.2.5",
    "@storybook/addons": "^5.2.5",
    "@storybook/cli": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "@types/canvasjs": "^1.9.5",
    "@types/chart.js": "^2.9.4",
    "@types/eslint": "^6.1.3",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/express": "^4.17.2",
    "@types/gh-pages": "^2.0.1",
    "@types/history": "^4.7.3",
    "@types/prettier": "^1.19.0",
    "@types/qs": "^6.9.0",
    "@types/query-string": "^6.3.0",
    "@types/redux-devtools": "^3.0.47",
    "@types/redux-logger": "^3.0.7",
    "@types/storybook__addon-info": "^5.2.1",
    "@types/uuid": "^3.4.6",
    "@types/yup": "^0.26.26",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "axios": "^0.19.0",
    "babel-plugin-styled-components": "^1.10.6",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-cypress": "^2.7.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.0.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "husky": "^3.0.9",
    "jest-styled-components": "^6.3.3",
    "lint-staged": "^9.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "react-docgen-typescript-loader": "^3.3.0",
    "redux-devtools": "^3.5.0",
    "redux-logger": "^3.0.6",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript-eslint": "^0.0.1-alpha.0"
  }
}
